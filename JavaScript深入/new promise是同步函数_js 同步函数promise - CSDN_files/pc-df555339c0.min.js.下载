$(function(){$(".baike-r .collect-a").on("click",function(){var i=getCookie("UserName");if(!i)return void window.csdn.loginBox.show();var n={url:window.location.href,title:baiduCollection.title,description:baiduCollection.desc,author:i,source_id:1,source:"GATHER"};window.csdn.collectionBox.show(n)})}),function(){$(document).ready(function(){$(".readmore_btn").on("click",function(){var i=$(this).data();if("blog"===i.soType)$(this).hide(),$(this).parent(".description").find(".digest").hide(),$(this).parent(".description").find(".content").show();else{var n=document.createElement("a");n.href=i.linkurl,n.target="_blank",n.click()}}),$(".hide-content a").on("click",function(){console.log($(this),$(this).parents(".desc-detail.content")),$(this).parents(".desc-detail.content").hide(),$(this).parents(".description").find(".desc-detail.digest").show(),$(this).parents(".description").find(".readmore_btn").show()}),$("#baike-intro>a").on("click",function(){$(this).parent().hide(),$("#baike-all").show(),$(".baike-container .hide-content").show(),$(".basic-info-wrap").show(),$(".basic-info-content ").show()}),$(".baike-container .hide-content").on("click",function(){$(this).hide(),$("#baike-intro").show(),$("#baike-all").hide(),$(".basic-info-wrap").hide(),$(".basic-info-content ").hide(),window.scrollTo(0,0)}),$(".content-list .desc-detail.content a").not(".hide-content a").attr("target","_blank")})}();