function getCookie(n){for(var t=document.cookie.split("; "),e=0;e<t.length;e++){var o=t[e].split("=");if(n==o[0])return o[1]}return null}function toolbarIsSuspension(){return!!window.csdn.configuration_tool_parameterv}window.csdn.configuration_tool_parameterv&&window.csdn.configuration_tool_parameterv({need_first_suspend:!0,need_little_suspend:!1,little_tool_id:"",little_need_insert_type:""});